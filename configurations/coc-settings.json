      "settings": {
        "Lua": {
          "workspace": {
            "library": {
              // "/path/to/hammerspoon-completion/build/stubs": true,
              "/opt/homebrew/Cellar/neovim/HEAD-fbe18d9_2/share/nvim/runtime/lua": true
              // "/Users/evan/.local/share/nvim/site/pack/packer/start/": true
            },
            "maxPreload": 10000,
